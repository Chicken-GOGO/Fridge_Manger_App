<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fridge_manger2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fridge_manger2">
        <activity
            android:name=".MainActivity2"
            android:exported="false"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.Fridge_manger2.NoActionBar" />
        <activity
            android:name=".FilterActivity"
            android:exported="false"
            android:label="@string/text_filter_title"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.Setting.appbar" />
        <activity
            android:name=".Onboarding"
            android:exported="true"
            android:theme="@style/Theme.Fridge_manger2.AppBarOverlay" />
        <activity
            android:name=".DesignActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SettingActivity"
            android:exported="false"
            android:label="@string/setting_bar_name"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.Setting.appbar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>